; ModuleID = 'LFortran'
source_filename = "LFortran"

@0 = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1

define void @print_int() {
.entry:
  %a = alloca i32, align 4
  store i32 5, i32* %a, align 4
  %0 = load i32, i32* %a, align 4
  call void (i8*, ...) @_lcompilers_printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @0, i32 0, i32 0), i32 %0)
  br label %return

return:                                           ; preds = %.entry
  ret void
}

declare void @_lcompilers_printf(i8*, ...)

define i32 @main() {
.entry:
  call void @print_int()
  ret i32 0
}
