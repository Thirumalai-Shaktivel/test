; ModuleID = 'LFortran'
source_filename = "LFortran"

@0 = private unnamed_addr constant [4 x i8] c"%f\0A\00", align 1
@1 = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1

define i32 @main() {
.entry:
  %i = alloca i32, align 4
  %r = alloca float, align 4
  store float 1.500000e+00, float* %r, align 4
  %0 = load float, float* %r, align 4
  %1 = fpext float %0 to double
  call void (i8*, ...) @_lcompilers_printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @0, i32 0, i32 0), double %1)
  %2 = load float, float* %r, align 4
  %3 = fptosi float %2 to i32
  store i32 %3, i32* %i, align 4
  %4 = load i32, i32* %i, align 4
  call void (i8*, ...) @_lcompilers_printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @1, i32 0, i32 0), i32 %4)
  ret i32 0
}

declare void @_lcompilers_printf(i8*, ...)
